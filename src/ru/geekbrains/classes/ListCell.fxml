<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="gridPane" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" maxHeight="Infinity" maxWidth="Infinity">
    <children>
        <HBox spacing="10">
            <children>
                <Label fx:id="date" textFill="darkgrey" prefWidth="60"/>
                <Label fx:id="message" HBox.hgrow="ALWAYS" wrapText="true" prefWidth="210"/>
            </children>
        </HBox>
    </children>
</GridPane>
